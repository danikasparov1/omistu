<odoo>
    <record id="view_quality_check_field_form" model="ir.ui.view">
        <field name="name">quality.check.field.form</field>
        <field name="model">quality.check.field</field>
        <field name="arch" type="xml">
            <form string="Quality Check Field">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_quality_check_field_tree" model="ir.ui.view">
        <field name="name">quality.check.field.tree</field>
        <field name="model">quality.check.field</field>
        <field name="arch" type="xml">
            <tree string="Quality Check Field">
                <field name="name"/>
            </tree>
        </field>
    </record>


    <record id="action_quality_check_field" model="ir.actions.act_window">
        <field name="name">Check Fields</field>
        <field name="res_model">quality.check.field</field>
        <field name="view_mode">tree,form</field>
         <field name="help" type="html">
            <p>Create new data</p>
        </field>
    </record>
</odoo>
