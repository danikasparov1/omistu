<odoo>
    <record id="view_res_config_settings_form" model="ir.ui.view">
        <field name="name">quality.config.settings</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="base.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//div[@class='settings']" position="inside">
                <div class="app_settings_block" string="Quality Assurance Settings">
                    <div>
                        <field name="external_test_report_enabled"/>
                    </div>
                </div>
            </xpath>
        </field>
    </record>
</odoo>
