<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="inventory_pb_stock_warehouse_form_inherit" model="ir.ui.view">
            <field name="name">ws_mgt</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="inside">
                    <page string="WS Access">
                        <tree>
                            <field name="allowed_warehouses_ids" options="{'no_create':1,'no_create_edit':1}"/>
                        </tree>
                    </page>
                </xpath>
            </field>
        </record>
    </data>





    
</odoo>