<odoo>
    <data>
        <!-- Payment Module Category -->
        <record model="ir.module.category" id="module_management_property_managment_new">
            <field name="name">Property</field>
            <field name="description">User access level for Property</field>
            <field name="sequence">3</field>
        </record>

        <!-- Payment User Group -->
        <record id="group_user_addis_employee_property_managemnt" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" ref="module_management_property_managment_new"/>
        </record>
        <record id="group_user_addis_systems_manager_property_managemnt" model="res.groups">
            <field name="name">Manager</field>
            <field name="category_id" ref="module_management_property_managment_new"/>
        </record>


        <record id="payment_request_user_own_record_rule" model="ir.rule">
            <field name="name">Access only self-created payment requests</field>
            <field name="model_id" ref="model_property_management" />
            <field name="domain_force">[('create_uid', '=', user.id)]</field>
            <field name="groups" eval="[(4, ref('property_management.group_user_addis_employee_property_managemnt'))]" />
        </record>
        
    </data>
</odoo>

  