<odoo>
    <record id="view_tender_valuation_commute_tree" model="ir.ui.view">
        <field name="name">tender.valuation.commute.tree</field>
        <field name="model">tender.valuation.commute</field>
        <field name="arch" type="xml">
            <tree>
                <field name="vendor_id"/>
                <field name="name"/>
                <field name="title"/>
            </tree>
        </field>
    </record>

    <record id="view_tender_valuation_commute_form" model="ir.ui.view">
        <field name="name">tender.valuation.commute.form</field>
        <field name="model">tender.valuation.commute</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="vendor_id"/>
                        <field name="name" readonly="1"/>
                        <field name="title"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

