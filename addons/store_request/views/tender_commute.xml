       <record id="view_employee_form_inherit_committee" model="ir.ui.view">
        <field name="name">hr.employee.form.inherit.committee</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="hr.view_employee_form"/>
        <field name="arch" type="xml">
           <xpath expr="//field[@name='coach_id']" position="after">
                <group>
                    <field name="is_committee"/>
                </group>
            </xpath>
        </field>
    </record>



    <record id="tender_valuation_commute_action" model="ir.actions.act_window">
        <field name="name">Tender Valuation Commute</field>
        <field name="res_model">tender.valuation.commute</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="tender_valuation_commute_submenu" name="Valuation Commutes" parent="tender_evaluation_criteria_submenu" action="tender_valuation_commute_action" sequence="10"/>
</odoo>